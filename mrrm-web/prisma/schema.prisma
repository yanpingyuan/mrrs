generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  UserId Int    @id @default(autoincrement())
  Name   String
  Email  String @unique
  CreateTime DateTime @default(now())
  IsAdmin Boolean @default(false)
  Phone String
  Password String
}
